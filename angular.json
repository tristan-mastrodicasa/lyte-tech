{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "lyte-tech": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "lt",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/lyte-tech",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.png",
              "src/assets",
              "src/manifest.webmanifest",
              "src/robots.txt"
            ],
            "styles": [
              "src/global.scss",
              "src/assets/cohost/css/open-iconic-bootstrap.min.css",
              "src/assets/cohost/css/animate.css",
              "src/assets/cohost/css/owl.carousel.min.css",
              "src/assets/cohost/css/owl.theme.default.min.css",
              "src/assets/cohost/css/magnific-popup.css",
              "src/assets/cohost/css/aos.css",
              "src/assets/cohost/css/ionicons.min.css",
              "src/assets/cohost/css/bootstrap-datepicker.css",
              "src/assets/cohost/css/jquery.timepicker.css",
              "src/assets/cohost/css/flaticon.css",
              "src/assets/cohost/css/icomoon.css"
            ],
            "scripts": [
              "src/assets/cohost/js/jquery.min.js",
              "src/assets/cohost/js/jquery-migrate-3.0.1.min.js",
              "src/assets/cohost/js/popper.min.js",
              "src/assets/cohost/js/bootstrap.min.js",
              "src/assets/cohost/js/jquery.easing.1.3.js",
              "src/assets/cohost/js/jquery.waypoints.min.js",
              "src/assets/cohost/js/jquery.stellar.min.js",
              "src/assets/cohost/js/owl.carousel.min.js",
              "src/assets/cohost/js/jquery.magnific-popup.min.js",
              "src/assets/cohost/js/aos.js",
              "src/assets/cohost/js/jquery.animateNumber.min.js",
              "src/assets/cohost/js/bootstrap-datepicker.js",
              "src/assets/cohost/js/scrollax.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "lyte-tech:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "lyte-tech:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "lyte-tech:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "lyte-tech:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "lyte-tech:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "lyte-tech"
}
